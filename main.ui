<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="tk.Frame" id="mainwindow">
    <property name="borderwidth">2</property>
    <property name="height">200</property>
    <property name="width">200</property>
    <layout>
      <property name="ipadx">5</property>
      <property name="ipady">5</property>
      <property name="padx">5</property>
      <property name="pady">5</property>
    </layout>
    <child>
      <object class="ttk.Label" id="Label_1">
        <property name="text" translatable="yes">来源时区:</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">4</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Scale" id="Scale_1">
        <property name="command">refreshWidgets</property>
        <property name="cursor">sb_h_double_arrow</property>
        <property name="from_">-12</property>
        <property name="length">320</property>
        <property name="orient">horizontal</property>
        <property name="to">12</property>
        <property name="value">0</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">5</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Checkbutton" id="Checkbutton_1">
        <property name="command">cb1_check</property>
        <property name="offvalue">False</property>
        <property name="onvalue">True</property>
        <property name="text" translatable="yes">使用夏令时</property>
        <property name="variable">boolean:cb1</property>
        <layout>
          <property name="column">3</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="Label_3">
        <property name="text" translatable="yes">目标时区:</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">4</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Scale" id="Scale_2">
        <property name="command">refreshWidgets</property>
        <property name="cursor">sb_h_double_arrow</property>
        <property name="from_">-12</property>
        <property name="length">320</property>
        <property name="orient">horizontal</property>
        <property name="to">12</property>
        <property name="value">0</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">4</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Checkbutton" id="Checkbutton_2">
        <property name="command">cb2_check</property>
        <property name="text" translatable="yes">使用夏令时</property>
        <property name="variable">boolean:cb2</property>
        <layout>
          <property name="column">3</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Spinbox" id="Spinbox_1">
        <property name="font">TkFixedFont</property>
        <property name="from_">00</property>
        <property name="increment">1</property>
        <property name="justify">center</property>
        <property name="text" translatable="yes">00</property>
        <property name="to">23</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">1</property>
          <property name="propagate">True</property>
          <property name="row">5</property>
          <property name="rowspan">1</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="Label_4">
        <property name="text" translatable="yes">:</property>
        <layout>
          <property name="column">1</property>
          <property name="columnspan">1</property>
          <property name="propagate">True</property>
          <property name="row">5</property>
          <property name="rowspan">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Spinbox" id="Spinbox_2">
        <property name="font">TkFixedFont</property>
        <property name="from_">00</property>
        <property name="increment">10</property>
        <property name="text" translatable="yes">00</property>
        <property name="justify">center</property>
        <property name="to">59</property>
        <layout>
          <property name="column">2</property>
          <property name="columnspan">1</property>
          <property name="propagate">True</property>
          <property name="row">5</property>
          <property name="rowspan">1</property>
          <property name="sticky">e</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="Button_1">
        <property name="command">calc_Button_clicked</property>
        <property name="default">normal</property>
        <property name="text" translatable="yes">计算</property>
        <layout>
          <property name="column">3</property>
          <property name="columnspan">4</property>
          <property name="propagate">True</property>
          <property name="row">5</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="Label_5">
        <property name="text" translatable="yes">结果：</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">6</property>
          <property name="padx">5</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="row">7</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Separator" id="Separator_1">
        <property name="orient">horizontal</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">5</property>
          <property name="ipadx">185</property>
          <property name="ipady">1</property>
          <property name="padx">1</property>
          <property name="pady">1</property>
          <property name="propagate">True</property>
          <property name="row">4</property>
          <property name="rowspan">1</property>
        </layout>
      </object>
    </child>
  </object>

</interface>
